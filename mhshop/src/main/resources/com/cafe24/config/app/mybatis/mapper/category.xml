<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="category">

	<!-- 카테고리 리스트 조회 -->
 	<insert id="selectList" parameterType="categoryvo">
	<![CDATA[
		select no, name from category order by no asc
	]]>
	</insert>
	
	
	
	
	
	
	
	
	
	
	
	<!-- 테스트 -->
	
	
	<!-- 테스트 insert -->
	<insert id="test_insert" parameterType="membervo">
	<![CDATA[
		insert into category(no, name)
		values(#{no}, #{name})
	]]>
	</insert>
	
	<!-- 테스트 deleteall -->
	<delete id="test_deleteall">
		delete from category
	</delete>
	
</mapper>
