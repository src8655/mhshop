<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="option">


	
	
	
	
	
	
	<!-- 테스트 -->
	
	
	<!-- 테스트 insert -->
	<insert id="test_insert" parameterType="categoryvo">
	<![CDATA[
		insert into option(no, item_no, option_detail1, option_detail2, cnt)
		values(#{no}, #{itemNo}, #{optionDetailNo1}, #{optionDetailNo2}, #{cnt})
	]]>
	</insert>
	
	<!-- 테스트 deleteall -->
	<delete id="test_deleteall">
		delete from option
	</delete>
	
</mapper>
